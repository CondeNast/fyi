  "homepage": "https://github.com/CondeNast/easy-fyi",
  "bugs": "https://github.com/CondeNast/easy-fyi/issues",
  "keywords": [
    "probot",
    "github",
    "probot-app"
  ],
  "scripts": {
    "dev": "nodemon --exec \"source authenticate-vault && npm start\"",
    "start": "PRIVATE_KEY=`node scripts/github.private.key.vault.js` probot run ./index.js",
    "lint": "standard --fix",
    "test": "standard --fix && jest --roots test",
    "integration_test": "standard --fix && jest --roots integration_test",
    "test:watch": "jest --watch --notify --notifyMode=change --coverage",
    "sequelize": "sequelize",
    "sequelize-db:migrate": "sequelize -- db:migrate"
  },
  "dependencies": {
    "@condenast/cn-vault": "^2.5.0",
    "config": "^1.30.0",
    "pg": "^7.4.3",
    "pg-hstore": "^2.3.2",
    "probot": "^6.0.0",
    "probot-commands": "^1.0.1",
    "probot-metadata": "^1.0.0",
    "smee-client": "^1.0.1",
    "sequelize": "^4.37.10",
    "sequelize-cli": "^4.0.0"
  },
  "devDependencies": {
    "jest": "^22.4.3",
    "nodemon": "^1.17.2",
    "pryjs": "^1.0.3",
    "standard": "^10.0.3"
  },
  "engines": {
    "node": ">= 8.3.0"
  },
  "standard": {
    "env": [
      "jest"
    ]
  }
}
